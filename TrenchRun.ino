void setup() {
  arduboy.begin();
  //beep.begin();
  arduboy.setFrameRate(FRAME_RATE);
}

void loop() {
  arduboy.clear();
  if(current_state == intro_state) {
    titleScreen();
  } else if(current_state == menu_state) {
    menu();
  } else if(current_state == warmup_state) {
    warmup();
  } else if(current_state == game_state) {
    gameLoop();
  } else if(current_state == win_state) {
    win();
  } else if(current_state == lose_state) {
    lose();
  }
  arduboy.display();
}

void titleScreen() {
  if(current_intro_state == text_state) {
    drawIntroText();
  } else if(current_intro_state == logo_state) {
    drawIntroLogo();
  } else if(current_intro_state == crawl_state) {
    drawIntroCrawl();
  }
}

void drawIntroText() {
  arduboy.setCursor(16,22);
  arduboy.setTextSize(2);
  arduboy.print("A long time ago in a galaxy far,");
  arduboy.print("\n");
  arduboy.print("far away...");
  arduboy.setTextSize(1);
  if(arduboy.everyXFrames(600)) {
    current_intro_state = logo_state;
  }
}

void drawIntroLogo() {
  Sprites::drawOverwrite(64,32,StarWarsLogo,introLogoFrame);
  if(arduboy.everyXFrames(60)) {
    if(introLogoFrame == 5) {
      current_intro_state = crawl_state;
    } else {
      introLogoFrame = introLogoFrame + 1;
    }
  }
}

void drawIntroCrawl() {
  arduboy.setCursor(16,introCrawlFrame);
  arduboy.setTextSize(2);
  arduboy.print("THE DEATH STAR");
  arduboy.setTextSize(1);
  arduboy.print("\n\n");
  arduboy.print("It's a dire time for the Rebels,\n");
  arduboy.print("Having their base on Yavin IV been discovered,\n");
  arduboy.print("The Death Star approaches and within minutes\n");
  arduboy.print("It will be ready to fire.\n\n");
  arduboy.print("The only hope rests on Red Squardron that now\n");
  arduboy.print("prepares to initiate their final attack...");
  if(arduboy.everyXFrames(20)) {
    //This can get awry
    introCrawlFrame = introCrawlFrame -1;
    if(introCrawlFrame == -64) {
      current_intro_state = text_state;
      current_state = menu_state;
    }
  }
}

void warmup() {
  if(lives == 0) {
    current_state = lose_state;
  } else {
    arduboy.setCursor(16,22);
    arduboy.print("Red 5, you're next,");
    arduboy.print("\n");
    arduboy.print("Prepare to Enter the Trench");
    arduboy.print("\n\n");
    arduboy.print("Remaining Lifes:");
    arduboy.print(lives);
    if(arduboy.everyXFrames(120)) {
      current_state = game_state;
    }
  }
}

void menu() {
   Sprites::drawOverwrite(64,32,StarWarsLogo,0);
   arduboy.setCursor(0,0);
   arduboy.print("Press the A button to start");
   if(arduboy.justPressed(A_BUTTON)) {
    current_state = warmup_state;
   }
}

//Refactor this win and lose functions
void win() {
   arduboy.setCursor(0,0);
   arduboy.print("You Win");
   if(arduboy.justPressed(A_BUTTON)) {
    current_state = intro_state;
   }
}

void lose() {
   arduboy.setCursor(0,0);
   arduboy.print("You Lose");
   if(arduboy.justPressed(A_BUTTON)) {
    current_state = intro_state;
   }
}


void gameLoop() {
  drawTrenchBackground();
  drawShipCockpit();
  drawCrosshair();
  moveCrosshair();
  decreaseDistance();
  drawDistance();
  checkDeath();
  
  //spawn tie fighters times remaining distance
  //if distance counter == 0 show exaust port image

  //if tie fighter too close, destroy it and damage player
  //if distance counter == 0 and pressed button within 1 second, show victory screen
}

void checkDeath() {
  if(hull == 0) {
    lives = lives -1;
    current_state = warmup_state;
  }
}

void decreaseDistance() {
  if(arduboy.everyXFrames(1) && currentDistanceFromExaustPort > 0) {
    currentDistanceFromExaustPort = currentDistanceFromExaustPort -1;
  }
}

void drawDistance() {
  arduboy.setCursor(0,0);
  arduboy.print(currentDistanceFromExaustPort);
}

void drawTrenchBackground() {
    trenchBackgroundSwap = !trenchBackgroundSwap;
//  Sprites::drawOverwrite(0,0,image,0);
}

void drawShipCockpit() {
//  Sprites::drawOverwrite(0,0,shipcockpit,0);
}

void drawCrosshair() {
  Sprites::drawOverwrite(crosshairx, crosshairy, CrosshairSprite, 0);
}

void moveCrosshair() {
  if (arduboy.pressed(LEFT_BUTTON)) {
      crosshairx = crosshairx - 1;
  }
  if (arduboy.pressed(RIGHT_BUTTON)) {
      crosshairx = crosshairx + 1;
  }
  if (arduboy.pressed(UP_BUTTON)) {
      crosshairy = crosshairy - 1;
  }
  if (arduboy.pressed(DOWN_BUTTON)) {
      crosshairy = crosshairy + 1;
  }
}
